<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.WorkDao">
    <insert id="addRecord" parameterType="entity.WorkEntity">
        INSERT INTO t_checkWork(sid,stime,longitude,latitude,state)
        VALUES (#{sid},#{stime},#{longitude},#{latitude},#{state});
    </insert>
</mapper>